lit_bool 
    = LIT_BOOL_FALSE
    {
        auto out = std::make_shared<ExprNode>();
        out->type = ExprType::LitBool;
        out->intValue = 0;
        return out;
    }
    | LIT_BOOL_TRUE
    {
        auto out = std::make_shared<ExprNode>();
        out->type = ExprType::LitBool;
        out->intValue = 1;
        return out;
    }
    ;