type 
    = type_name OPEN_SQUARE LIT_INT CLOSE_SQUARE
    {
        auto out = std::make_shared<TypeNode>();
        out->type = Type::Array;
        out->subtype = std::static_pointer_cast<TypeNode>($type_name);
        return out;
    }
    | type_name
    ;

type_name
    = TYPE_INT
    {
        auto out = std::make_shared<TypeNode>();
        out->type = Type::Int;
        return out;
    }
    | TYPE_STR
    {
        auto out = std::make_shared<TypeNode>();
        out->type = Type::Str;
        return out;
    }
    | TYPE_BOOL
    {
        auto out = std::make_shared<TypeNode>();
        out->type = Type::Bool;
        return out;
    }
    ;
